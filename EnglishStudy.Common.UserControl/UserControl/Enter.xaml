<UserControl x:Class="EnglishStudy.Common.UserControl.UserControl.Enter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnglishStudy.Common.UserControl.UserControl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="{DynamicResource GridStyle}">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox Margin="20" MinWidth="200"></TextBox>
            <TextBox Margin="20" MinWidth="200"></TextBox>
        </StackPanel>  
    </Grid>
</UserControl>
